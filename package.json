{
    "name": "node-wxcrypto",
    "description": "微信消息加解密nodejs版本",
    "version": "1.0.0-beta.0",
    "packageManager": "pnpm@7.17.0",
    "main": "lib/index.cjs",
    "module": "lib/index.mjs",
    "exports": {
        ".": {
            "require": "./lib/index.cjs",
            "import": "./lib/index.mjs"
        },
        "./lib": "./lib/index.cjs",
        "./*": "./*"
    },
    "files": [
        "es",
        "lib",
        "typings"
    ],
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src"
    },
    "scripts": {
        "build": "rimraf lib es && rollup -c",
        "build:types": "run-s build-temp-types roll-types",
        "build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p src/",
        "patch-types": "node script/patchTypes",
        "roll-types": "api-extractor run && rimraf temp",
        "dev": "rollup--config ./rollup.config.ts --configPlugin typescript -c -w ",
        "pub": "tscjs scripts/publish",
        "unpub": "tscjs scripts/unpublish",
        "sync": "tscjs scripts/sync",
        "dist": "run-s eslint prettier build build:types docs prettier:docs",
        "test-unit": "jest --filter ./scripts/filter-unit.js",
        "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
        "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "docs": "rimraf docs && typedoc",
        "eslint": "eslint --fix .",
        "prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\"",
        "prettier:docs": "prettier --write \"**/*.md\""
    },
    "dependencies": {
        "chalk": "^5.1.2",
        "consola": "^2.15.3",
        "core-js": "^3.26.0",
        "debug": "^4.3.4",
        "fs-extra": "^11.1.0",
        "js-cool": "^2.5.0",
        "ora": "^6.1.2",
        "picocolors": "^1.0.0",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.2",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.20.2",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@eslint-sets/eslint-config-simple-ts": "^3.0.5",
        "@microsoft/api-extractor": "^7.33.5",
        "@rollup/plugin-babel": "^6.0.2",
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^29.2.2",
        "@types/node": "^18.11.9",
        "@types/xml2js": "^0.4.11",
        "babel-loader": "^9.1.0",
        "coveralls": "^3.1.1",
        "cross-env": "^7.0.3",
        "esbuild": "^0.16.3",
        "eslint": "^8.27.0",
        "jest": "^29.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "prettier-config-common": "^1.2.1",
        "rimraf": "^3.0.2",
        "rollup": "^3.9.1",
        "rollup-plugin-typescript2": "^0.34.1",
        "rollup-plugin-visualizer": "^5.8.3",
        "ts-jest": "^29.0.3",
        "tsnd": "^1.1.0",
        "typedoc": "^0.23.20",
        "typedoc-plugin-markdown": "^3.13.6",
        "typescript": "^4.8.4"
    },
    "peerDependencies": {
        "core-js": ">= 3",
        "js-cool": ">= 2",
        "xml2js": ">= 0.4",
        "debug": ">= 4"
    },
    "keywords": [
        "wx-decrypt",
        "msg-decrypt",
        "xml-decrypt",
        "decryptXML",
        "wx-crypto",
        "extension",
        "node-wxcrypto",
        "wxcrypto",
        "saqqdy"
    ],
    "license": "MIT",
    "author": "saqqdy <https://github.com/saqqdy>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/node-wxcrypto.git"
    },
    "bugs": {
        "url": "https://github.com/saqqdy/node-wxcrypto/issues"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "tslib",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.0.0"
            }
        }
    },
    "homepage": "https://github.com/saqqdy/node-wxcrypto#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
