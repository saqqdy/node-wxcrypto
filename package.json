{
	"name": "node-wxcrypto",
	"description": "微信消息加解密nodejs版本",
	"version": "1.0.0",
	"packageManager": "pnpm@7.25.0",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"typings": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./lib": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./es": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./lib/*",
			"import": "./es/*"
		}
	},
	"files": [
		"dist",
		"es",
		"lib",
		"typings"
	],
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src"
	},
	"scripts": {
		"build": "run-s clean build:{bundle,module}",
		"build:module": "run-s build:{esm,cjs} roll-types",
		"build:bundle": "rollup -c build/rollup.config.ts --configPlugin @rollup/plugin-typescript",
		"build:esm": "tsc -b --force tsconfig.esm.json",
		"build:cjs": "tsc -b --force tsconfig.cjs.json",
		"roll-types": "api-extractor run",
		"watch": "pnpm build:bundle -w",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"clean": "rimraf lib es dist",
		"dist": "run-s clean build docs prettier:docs",
		"test-unit": "jest --filter ./scripts/filter-unit.js",
		"jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
		"coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"docs": "rimraf docs && typedoc",
		"eslint": "eslint --fix .",
		"prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\"",
		"prettier:docs": "prettier --write \"**/*.md\""
	},
	"dependencies": {
		"chalk": "^5.2.0",
		"consola": "^2.15.3",
		"core-js": "^3.27.1",
		"debug": "^4.3.4",
		"fs-extra": "^11.1.0",
		"js-cool": "^2.8.0",
		"ora": "^6.1.2",
		"picocolors": "^1.0.0",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@eslint-sets/eslint-config-ts": "^4.1.0",
		"@microsoft/api-extractor": "^7.33.7",
		"@rollup/plugin-alias": "^4.0.2",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-typescript": "^11.0.0",
		"@types/fs-extra": "^11.0.1",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/xml2js": "^0.4.11",
		"babel-loader": "^9.1.2",
		"coveralls": "^3.1.1",
		"cross-env": "^7.0.3",
		"esbuild": "^0.17.0",
		"eslint": "^8.32.0",
		"jest": "^29.3.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"prettier-config-common": "^1.3.0",
		"rimraf": "^4.0.7",
		"rollup": "^3.10.0",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-visualizer": "^5.9.0",
		"ts-jest": "^29.0.5",
		"tsnd": "^1.1.0",
		"typedoc": "^0.23.24",
		"typedoc-plugin-markdown": "^3.14.0",
		"typescript": "^4.9.4"
	},
	"peerDependencies": {
		"core-js": ">= 3",
		"debug": ">= 4",
		"js-cool": ">= 2",
		"xml2js": ">= 0.4"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"tslib",
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	},
	"keywords": [
		"wx-decrypt",
		"msg-decrypt",
		"xml-decrypt",
		"decryptXML",
		"wx-crypto",
		"extension",
		"node-wxcrypto",
		"wxcrypto",
		"saqqdy"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/node-wxcrypto#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/node-wxcrypto/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/node-wxcrypto.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
